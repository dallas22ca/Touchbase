<h1>Listing websiteships</h1>

<table>
  <thead>
    <tr>
      <th>Website</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @websiteships.each do |websiteship| %>
      <tr>
        <td><%= websiteship.website %></td>
        <td><%= websiteship.user %></td>
        <td><%= link_to 'Show', websiteship %></td>
        <td><%= link_to 'Edit', edit_websiteship_path(websiteship) %></td>
        <td><%= link_to 'Destroy', websiteship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Websiteship', new_websiteship_path %>
