$("#import_progress").animate({
  width: "<%= @user.import_progress %>%"
}, 5000)
$("#import_progress").attr("data-progress", <%= @user.import_progress %>)
$(".contacts_count").text("<%= pluralize @user.contacts_count, "Contact" %>")
$(".contacts_search_count").text("<%= @user.contacts_count %>")
